{
  "name": "bowel-buddies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:firebase-sw": "node scripts/generate-firebase-sw.js",
    "dev": "npm run generate:firebase-sw && next dev",
    "build": "npm run generate:firebase-sw && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest && jest --config jest.config.api.js",
    "test:api": "jest --config jest.config.api.js",
    "test:watch": "jest --watch",
    "build:worker": "opennextjs-cloudflare build",
    "dev:worker": "wrangler dev --config wrangler.json --port 8787",
    "preview": "npm run build:worker && wrangler dev --config wrangler.json",
    "deploy": "npm run build:worker && wrangler pages deploy .open-next --project-name=bowel-buddies"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "firebase": "^12.9.0",
    "h3-js": "^4.4.0",
    "jose": "^6.1.3",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "next": "15.3.9",
    "next-pwa": "^5.6.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "stripe": "^20.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.9",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "puppeteer": "^24.37.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^47.0.4",
    "wrangler": "^4.63.0"
  }
}
